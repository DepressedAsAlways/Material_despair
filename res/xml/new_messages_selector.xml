<?xml version="1.0" encoding="utf-8"?>
<selector android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <nine-patch android:src="@drawable/newmessageon" android:tint="@color/kinky_secondary_white" />
    </item>
    <item android:state_focused="true">
        <nine-patch android:src="@drawable/newmessageon" android:tint="@color/kinky_secondary_white" />
    </item>
    <item android:state_pressed="true">
        <nine-patch android:src="@drawable/newmessageon" android:tint="@color/kinky_secondary_white" />
    </item>
    <item>
        <nine-patch android:src="@drawable/newmessage" android:tint="@color/kinky_secondary_black" />
    </item>
</selector>