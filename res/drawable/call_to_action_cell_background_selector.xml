<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
        <nine-patch android:src="@drawable/card_white" android:tint="@color/kinky_secondary_white" />
    </item>
	<item android:state_focused="true">
        <nine-patch android:src="@drawable/card_white" android:tint="@color/kinky_secondary_white" />
    </item>
	<item android:state_selected="true">
        <nine-patch android:src="@drawable/card_white" android:tint="@color/kinky_secondary_white" />
    </item>
    <item>
        <nine-patch android:src="@drawable/card_white" android:tint="@color/kinky_main" />
    </item>
</selector>