<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <bitmap android:src="@drawable/ic_chat_info_edit_photo" android:tint="@color/kinky_secondary_black" />
    </item>
    <item android:state_focused="true">
        <bitmap android:src="@drawable/ic_chat_info_edit_photo" android:tint="@color/kinky_secondary_black" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/ic_chat_info_edit_photo" android:tint="@color/kinky_secondary_black" />
    </item>
    <item>
        <bitmap android:src="@drawable/ic_chat_info_edit_photo" android:tint="@color/kinky_main" />
    </item>
</selector>